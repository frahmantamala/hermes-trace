{
  "name": "hermes-trace",
  "version": "1.0.0",
  "type": "module",
  "description": "Framework-agnostic frontend logging package with support for multiple transports (Loki, Datadog, Sentry)",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./vue": {
      "import": "./dist/vue.esm.js",
      "require": "./dist/vue.js",
      "types": "./dist/vue.d.ts"
    },
    "./angular": {
      "import": "./dist/angular.esm.js",
      "require": "./dist/angular.js",
      "types": "./dist/angular.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "demo": "python3 -m http.server 8080 --directory demo",
    "demo:win": "python -m http.server 8080 --directory demo",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "logging",
    "frontend",
    "universal",
    "react",
    "vue",
    "angular",
    "loki",
    "datadog",
    "sentry",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "rollup": "^4.6.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "@sentry/browser": "^7.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@sentry/browser": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
